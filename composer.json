{
  "name": "rodziu/php-git",
  "description": "Pure PHP Git client (read only).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Rohde",
      "email": "mateusz.rohde@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rodziu\\Git\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rodziu\\Git\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "ext-curl": "*",
    "ext-zlib": "*"
  },
  "require-dev": {
    "ext-zip": "*",
    "phpunit/phpunit": "^10.5.9",
    "friendsofphp/php-cs-fixer": "^3.53",
    "phpstan/phpstan": "^1.10"
  }
}
